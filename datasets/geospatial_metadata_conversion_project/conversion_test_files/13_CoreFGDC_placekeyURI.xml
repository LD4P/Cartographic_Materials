<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE metadata SYSTEM "http://www.fgdc.gov/metadata/fgdc-std-001-1998.dtd">
<!-- <@layerid> is a Local Identifier and is unique to this layer <@hollisno> is a Local Identifier corresponding to HOLLIS number, non-unique-->
<metadata layerid="CAMBRIDGE14PUBLICLIBRARIES" hollisno="014266502">
	<!-- Developmental FGDC xml record. Includes core ontology concepts: Local Identifiers, Title, 
		Originators Activity and Publication Activity, online link, edition, thematic keywords and place keywords,
		and 1 geospatial extension ontology concept: bounding box, Presentation form. 
		
		Added concepts to this version: None. Source file to test placekeys to geonames concordance mappings.
		 -->
	<idinfo>
		<citation>
			<citeinfo>
				<!-- Each <origin> should have its own Originator activity -->
				<origin>Cambridge (Mass.). Geographic Information Systems</origin> <!-- in concordance file -->
				<origin>Cambridge (Mass.). Public Library</origin> <!-- NOT in concordance file -->
				<!-- <pubdate> is part of Publication activity -->
				<pubdate>2014</pubdate>
				<title>Public Libraries, Cambridge, Massachusetts, 2014</title>
				<!-- geospatial data presentation form mapped to cartographic Work subtypes -->
				<geoform>geoform test value</geoform> <!-- NOT in concordance file --> 
				<edition>2014 revised ed.</edition>
				<!-- <pubplace> and <publish> are part of Publication activity -->
				<pubinfo>
					<pubplace>Cambridge, Massachusetts</pubplace> <!-- in concordance file -->
					<publish>Cambridge GIS</publish> <!-- in concordance file -->
				</pubinfo>
				<!-- onlink corresponds to the item electronicLocator -->
				<onlink>http://hgl.harvard.edu:8080/HGL/hgl.jsp?action=VColl&amp;VCollName=CAMBRIDGE14PUBLICLIBRARIES</onlink>
			</citeinfo>
		</citation>
		<descript>
			<!-- <abstract> and <summary> go into separate Annotations -->
			<abstract>This layer contains point features of all public libraries in Cambridge.</abstract>
			<purpose>Created for general use by City staff. Used for both mapping and specific identification of buildings with a similar use.</purpose>
		</descript>
		<spdom>
			<bounding>
				<!-- bounding box elements need to be converted to a single WKT statement -->
				<westbc>-71.146912</westbc>
				<eastbc>-71.084684</eastbc>
				<northbc>42.392720</northbc>
				<southbc>42.363838</southbc>
			</bounding>
		</spdom>
		<!-- Keywords section -->
		<keywords>
			<!-- Thematic keywords; <theme> sections can repeat; used here with 3 different thesauri sources:  LCSH, ISO and None  -->
			<theme>
				<themekt>LCSH</themekt>
				<themekey>Libraries</themekey> <!-- NOT in concordance file -->
				<themekey>Public libraries</themekey> <!-- in concordance file -->
			</theme>
			<theme>
				<themekt>ISO 19115 Topic Category</themekt>
				<themekey>structure</themekey> <!-- in concordance file -->
			</theme>
			<theme>
				<themekt>None</themekt>
